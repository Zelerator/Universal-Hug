                ["[6] Zeus"] = CFrame.new(15797, -58.5, 1893.5),
                ["[7] Cronus"] = CFrame.new(18437, -61, 1893.5),
                ["[8] Robots"] = CFrame.new(20560, -56.9952087, 2152),
